<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" onKeyTyped="#keyPressed" prefHeight="170.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.raktar.DelGoodsController">
   <children>
      <VBox prefHeight="142.0" prefWidth="250.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Törlendő termék kategóriája:">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Label>
            <ComboBox fx:id="itemCategory" onAction="#selectedCategory" prefWidth="351.0" promptText="Válasszon kategóriát...">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </ComboBox>
            <Label fx:id="delGoodsNameLabel" text="Törlendő termék neve:">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Label>
            <ComboBox fx:id="itemName" layoutX="25.0" layoutY="25.0" onAction="#selectedGoods" prefWidth="351.0" promptText="Válasszon terméket...">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </ComboBox>
            <HBox alignment="TOP_CENTER" layoutX="20.0" layoutY="295.0" prefHeight="26.0" prefWidth="249.0">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <children>
                  <Button fx:id="cancelBtn" contentDisplay="CENTER" mnemonicParsing="false" onAction="#cancel" text="Mégsem">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="delBtn" contentDisplay="CENTER" mnemonicParsing="false" onAction="#del" prefHeight="26.0" prefWidth="62.0" text="Törlés" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
